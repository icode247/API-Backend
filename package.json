{
  "name": "hiroek",
  "version": "1.0.0",
  "description": "Hiroek API Server",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc && cp -r ./src/templates/ ./build/templates",
    "prod-build": "npm run build",
    "dev": "nodemon",
    "start-notification-worker": "node build/notificationWorker.js",
    "start": "node build/app.js",
    "inspect": "nodemon --inspect src/app.ts",
    "test": "jest",
    "lint": "npm run lint:js ",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npm run lint:js -- --fix",
    "seed:admin": "npm run build && node build/seeders/seedAdmin.js",
    "seed:splitDataset": "npm run build && cp ./src/seeders/dataset1.json build/seeders/ && cp ./src/seeders/dataset2.json build/seeders/ && cp ./src/seeders/dataset3.json build/seeders/ && node build/seeders/splitDatasets.js",
    "seed:organizations": "npm run seed:splitDataset && node build/seeders/seedOrganizations.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Heroik-Foundation/API-Backend.git"
  },
  "keywords": [
    "js",
    "javascript",
    "typescript",
    "node",
    "express"
  ],
  "author": "Confidence Yobo",
  "license": "ISC",
  "dependencies": {
    "argon2": "^0.27.0",
    "aws-sdk": "^2.1328.0",
    "axios": "^1.3.4",
    "axios-retry": "^3.6.0",
    "body-parser": "^1.19.0",
    "celebrate": "^13.0.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.9",
    "errorhandler": "^1.5.1",
    "event-dispatch": "^0.4.1",
    "eventemitter3": "^4.0.7",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-jwt": "^8.4.1",
    "form-data": "^2.3.3",
    "helmet": "^6.0.1",
    "JSONStream": "^1.3.5",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mailgun.js": "8.2.1",
    "method-override": "^3.0.0",
    "moment": "^2.29.0",
    "moment-timezone": "^0.5.31",
    "mongoose": "^5.10.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-cache": "^5.1.2",
    "redis": "^4.6.5",
    "reflect-metadata": "^0.1.13",
    "rollbar": "^2.26.1",
    "socket.io": "^4.6.1",
    "stripe": "^11.17.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "twilio": "^4.8.0",
    "typedi": "^0.8.0",
    "uuid": "^9.0.0",
    "winston": "^3.3.3",
    "winston-transport": "^4.4.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.161",
    "@types/mongoose": "^5.7.36",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^14.11.2",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "supertest": "^6.3.3",
    "ts-jest": "^26.4.0",
    "ts-mockito": "^2.6.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.11.0",
    "tslint": "^5.20.1",
    "typescript": "^4.0.3"
  }
}
